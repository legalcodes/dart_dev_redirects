{
  "hosting": {
    "public": "_site",
    "cleanUrls": true,
    "trailingSlash": false,
    "headers": [{
        "source": "**/*.@(txt|json)",
        "headers": [ { "key": "Access-Control-Allow-Origin", "value": "*" } ]
    }],
    "redirects": [
      { "source": "/angular", "destination": "http://webdev.dartlang.org/angular", "type": 301 },
      { "source": "/angular/:path*", "destination": "http://webdev.dartlang.org/angular/:path", "type": 301 },
      { "source": "/:path*", "destination": "https://dartlang-org-staging-0.firebaseapp.com/:path", "type": 301 }

    ]
  }
}
